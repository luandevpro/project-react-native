{
   "extends": "airbnb",
   "parser": "babel-eslint",
   "plugins": ["react"],
   "rules": {
      "react/jsx-filename-extension": [1, {
         "extensions": [".js", ".jsx"]
      }],
      "react/prop-types": [2, {
         "ignore": ["children"]
      }],
      "react/no-unused-state": "error"
   }
}